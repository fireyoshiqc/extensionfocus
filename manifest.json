{
  "manifest_version": 2,

  "name": "Focus on Messenger",
  "description": "Cette extension permet de changer les couleurs de Messenger!",
  "version": "0.1",

  
  "content_scripts": [
    {
      "matches": ["https://www.messenger.com/*"], 
      "css": ["styles.css"],
      "js": ["cssScript.js"]
    }
],
 "browser_action": {
          "default_icon": "icon.png",
		  "default_title": "Focus on Messenger"
 },
		  
  
 
  "permissions": [
	"tabs", "https://www.messenger.com/*"
  ],
   "web_accessible_resources": ["styles.css"]
}
